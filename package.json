    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "node-fetch": "^1.6.3",
    "nodemon": "^1.11.0",
    "query-string": "^4.3.2",
    "ramda": "^0.23.0",
    "request": "^2.81.0"
  }
}
